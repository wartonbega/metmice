

struct random
{
    31415821 -> a;
    10^8 -> m;
    !time() -> seed;
    
    randint(self)[none]
    {
        !get_compute_mode() -> mode;
        !set_compute_mode(1);

        self.a * self.seed + 1 -> r;
        r % self.m -> r;

        r -> self.seed;
        !set_compute_mode(mode);
        
        return r;
    };
};