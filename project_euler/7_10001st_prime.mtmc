include "../lib/std.mtmc";


premier(x)[none]
{
    2 -> i;
    while (i^2 < x)
    {
        if (x%i = 0)
        {
            return false;
        };
        i + 1 ->i;
    };  
    return true;
};  


main()[none]
{
    0 -> count;
    3 -> i;
    while (!not(count = 10001))
    {
        !print(count);
        if(!premier(i))
        {  
            count + 1 -> count;
        };
        i+1 -> i;
    };
    !print(i);
};


