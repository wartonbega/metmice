include "../lib/std.mtmc";


divisible(n)[none]
{
    for(i, 1, 21)
    {
        if (!not(n % i = 0))
        {
            return false;
        };
    };
    return true;
};

main()[none]
{
    21 -> n;
    while(true)
    {
        if (!divisible(n))
        {
            push 1 "-->";
            !print(n);
            return n;
        };
        !print(n);
        n + 1 -> n;
    };
};
!main();